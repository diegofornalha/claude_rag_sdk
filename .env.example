# =============================================================================
# RAG Agent - Environment Variables (Template)
# =============================================================================
# Copie este arquivo para .env e configure os valores
# cp .env.example .env
# =============================================================================

# API Key para autenticacao
# Se nao definida, sera gerada automaticamente na primeira execucao
RAG_API_KEY=

# Ambiente (development/production)
ENVIRONMENT=development

# Autenticacao habilitada (true/false)
AUTH_ENABLED=true
