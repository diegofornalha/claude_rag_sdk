{
  "adapters": {
    "angular-cli": {
      "enabled": true,
      "command": ["npx", "-y", "@angular/cli", "mcp"],
      "timeout_seconds": 60.0,
      "options": {
        "read_only": false,
        "local_only": false
      }
    }
  },
  "default_timeout": 30.0,
  "max_concurrent_connections": 5,
  "ingest_delay_between_calls": 0.5,
  "ingest_batch_size": 10
}
